{
  "name": "generic-blog",
  "version": "0.1.0",
  "description": "A generic blog server written in NodeJS with OpenAPI documentation",
  "sideEffects": false,
  "scripts": {
    "start": "node dist/app.min.js",
    "start:dev": "webpack --config webpack.dev.config.js --watch",
    "start:client": "webpack-dev-server --config webpack.client.config.js --mode development",
    "build": "npm run build:server && npm run build:client",
    "build:server": "webpack --config webpack.prod.config.js",
    "build:client": "webpack --config webpack.client.config.js",
    "test": "node server/runTests.js"
  },
  "author": "Tim McCabe",
  "dependencies": {
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "express-openapi": "^3.0.2",
    "json-sql": "^0.4.11",
    "jsonwebtoken": "^8.3.0",
    "morgan": "^1.9.1",
    "pg": "^7.5.0",
    "pg-format": "^1.0.4",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "source-map-support": "^0.5.9",
    "swagger-client": "^3.8.21",
    "swagger-ui-express": "^4.0.1",
    "webpack-closure-compiler": "^2.1.6"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.4",
    "clean-webpack-plugin": "^0.1.19",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "nodemon-webpack-plugin": "^3.0.1",
    "swagger-js-codegen": "^1.12.0",
    "val-loader": "^1.1.1",
    "webpack": "^4.22.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10",
    "webpack-node-externals": "^1.7.2"
  }
}
