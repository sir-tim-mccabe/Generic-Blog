{
  "name": "generic-blog",
  "version": "0.1.0",
  "description": "A generic blog server written in NodeJS with OpenAPI documentation",
  "scripts": {
    "start": "node dist/app.min.js",
    "start:dev": "nodemon --exec \"node debug/app.min.js\"",
    "build": "webpack --config webpack.prod.config.js",
    "build:dev": "webpack --config webpack.dev.config.js",
    "watch": "webpack --config webpack.dev.config.js --watch",
    "test": "node server/runTests.js"
  },
  "author": "Tim McCabe",
  "dependencies": {
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "express-openapi": "^3.0.2",
    "json-sql": "^0.4.11",
    "jsonwebtoken": "^8.3.0",
    "morgan": "^1.9.1",
    "pg": "^7.5.0",
    "pg-format": "^1.0.4",
    "swagger-client": "^3.8.21",
    "swagger-ui-express": "^4.0.1"
  },
  "devDependencies": {
    "file-loader": "^2.0.0",
    "nodemon": "^1.18.4",
    "webpack": "^4.22.0",
    "webpack-cli": "^3.1.2",
    "webpack-closure-compiler": "^2.1.6",
    "webpack-node-externals": "^1.7.2"
  }
}
